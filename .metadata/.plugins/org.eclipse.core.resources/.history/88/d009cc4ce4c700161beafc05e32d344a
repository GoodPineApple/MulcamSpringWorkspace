<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="repository.mapper.MemberMapper">
	<insert id="insertMember" parameterType="Member">
		INSERT INTO MEMBER(ID,PASSWORD,EMAIL,PHONE)
		VALUES(#{id}, #{password}, #{email}, #{phone})
	</insert>
	
	<select id="selectMember" parameterType="Integer" resultMap="memberMap">
		SELECT * FROM MEMBER WHERE MEMBER_NUM=#{memberNum}
	</select>
	
	
	<resultMap type="Member" id="memberMap">
		<result column="member_num" property="memberNum"/>
		<result column="id" property="id"/>
		<result column="password" property="password"/>
		<result column="email" property="email"/>
		<result column="phone" property="phone"/>
	</resultMap>
	
</mapper>